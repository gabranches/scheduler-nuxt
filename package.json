    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "firebase": "^5.6.0",
    "jquery": "^3.3.1",
    "js-sha256": "^0.9.0",
    "leaflet": "^1.3.4",
    "lodash": "^4.17.11",
    "nuxt": "^2.0.0",
    "nuxt-leaflet": "0.0.10",
    "vue2-leaflet": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.10.0",
