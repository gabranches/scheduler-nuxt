{
  "name": "nuxt-template",
  "version": "1.0.0",
  "description": "A template for Nuxt",
  "author": "Gustavo Abranches",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
